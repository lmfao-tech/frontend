
generator client {
  provider = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

model Like {
  id           String    @id
  user         User[]
}

model User {
  id           String    @id
  name         String
  email        String    @unique
  hahaCoins    Int       @default(0)
  lmfaoCoins   Int       @default(0)
  likes        Like[]
}